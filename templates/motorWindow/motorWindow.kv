#:kivy 1.0

<MotorWindow>:
    name: "motor"

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        Label:
            text: "Motor Screen"
            font_size: 32
        
        Button:
            text: "Go Back"
            font_sice: 32
            on_release: 
                app.root.current = "motorDataSetting"
                root.manager.transition.direction = "left"
